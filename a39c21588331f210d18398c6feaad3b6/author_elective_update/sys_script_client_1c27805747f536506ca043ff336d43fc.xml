        <description>Trigger instantly when the user changes floor or facility &amp; uses g_form.getReference() to fetch related records already available in the form context then updates dependent fields immediately, no save required to see changes.</description>
        <field>complex</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>FMC_Complex &amp; FMC Fields- Client</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue === '') return;

    // Get the fmc record
    g_form.getReference('FMC', function(FMCRec) {
        if (!FMCRec) return;

        // Set complex from fmc
        if (fmcRec.complex) {
            g_form.setValue('complex', fmcRec.complex);

                }
            });
        }

]]></script>
