    <sys_transform_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <map display_value="PropertyTable">34b349d64792be106ca043ff336d43bd</map>
        <order>100</order>
        <script><![CDATA[(function runTransformScript(source, map, log, target) {

  // 1) Get the incoming Complex ID from your staging row
  var complexId = (source.u_complex_id || '').toString().trim();
  if (!complexId) {
    log.warn('Missing Complex ID on source row: ' + source.sys_id);
    return;
  }

  // 2) Look up the Complex by its Complex ID
  var cpx = new GlideRecord('x_1831875_amp2_0_x_amp2_fmc_complex');
  cpx.addQuery('u_complex_id', complexId);
  cpx.setLimit(1);
  cpx.query();

  if (cpx.next()) {
    // 3) Set the reference on the Property record
    target.u_complex = cpx.getUniqueValue();
  } else {
    log.warn('No Complex match found for Complex ID [' + complexId + '] on source row: ' + source.sys_id);
  }

})(source, map, log, target);

]]></script>
