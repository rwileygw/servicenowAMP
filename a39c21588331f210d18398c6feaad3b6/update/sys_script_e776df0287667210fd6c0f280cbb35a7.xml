        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var common = (current.prop_common_name || '').toString().trim();
    var official = (current.u_property_name || '').toString().trim();

    if (common && official && common !== official) {
        current.u_display_label = common + '(' + official + ')';
    } else if (common) {
        current.u_display_label = common;
    } else {
        current.u_display_label = official;
    }

})(current, previous);]]></script>
