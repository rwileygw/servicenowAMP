<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>This Client script multiplies the Ranking Factor score from the Drop down field by 100 to set the value of the Ranking Factor Score field.</description>
        <field>ranking_factors</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name> RankingFactorScore</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   var val = parseFloat(newValue);
   if (isNaN(val)) {
	g_form.clearValue('risk_score');
	return;

   }
   g_form.setValue('risk_score', val * 100);
   
}]]></script>
