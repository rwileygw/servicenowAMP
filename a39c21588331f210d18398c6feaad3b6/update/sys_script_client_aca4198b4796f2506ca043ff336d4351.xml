        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   //Type appropriate comment here, and begin script below

   g_form.getReference('u_property', function(prop) {
	if (!prop) return;

	g_form.setValue('u_city', prop.u_city);
	g_form.setValue('u_state', prop.u_state);
	g_form.setValue('u_zip', prop.u_zip);
	g_form.setValue('u_property_id', prop.u_property_id);

	if (prop.u_complex) {
		g_form.setValue('u_complex', prop.u_complex);
	}
   });
   
}]]></script>
