        <name>Calculate Total Funds -  FMC</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading) return;
    
    function toInt(v) {
        if (!v) return 0;
        //remove commas, dollar signs, spaces
        v = ('' + v).replace(/[^0-9\-]/g, '');
        var n = parseInt(v, 10);
        return isNan(n) ? 0 : n;

    }
    var fmc = toInt(g_form.getValue('u_fmc_funds'));
    var hq = toInt(g_form.getValue('u_hq_funds'));
    var other = toInt(g_form.getValue('u_other_funds'));

    g_form, setValue('u_total_funds', fmc + hq + other);

}]]></script>
